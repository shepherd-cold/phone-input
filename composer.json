{
    "name": "shepherd-cold/phone-input",
    "description": "Telephone Input component modified by Shepherd",
    "keywords": [
        "laravel",
        "livewire"
    ],
    "homepage": "https://github.com/shepherd-cold/phone-input",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Victory Osayi Airuoyuwa",
            "email": "lavictorybiz@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Victorybiz\\LaravelTelInput\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Victorybiz\\LaravelTelInput\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Victorybiz\\LaravelTelInput\\LaravelTelInputServiceProvider"
            ],
            "aliases": {
                "LaravelTelInput": "Victorybiz\\LaravelTelInput\\LaravelTelInputFacade"
            }
        }
    }
}
